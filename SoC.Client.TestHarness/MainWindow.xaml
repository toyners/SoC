<Window x:Class="Client.TestHarness.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client.TestHarness"
        mc:Ignorable="d"
        Title="SoC TestHarness" Height="640" Width="800">
  <Grid>
    <Grid.ColumnDefinitions>
      <ColumnDefinition />
      <ColumnDefinition />
    </Grid.ColumnDefinitions>

    <Grid.RowDefinitions>
      <RowDefinition />
      <RowDefinition />
    </Grid.RowDefinitions>

    <local:ClientUI />
    <local:ClientUI Grid.Column="1"/>
    <local:ClientUI Grid.Row="1"/>
    <local:ClientUI Grid.Row="1" Grid.Column="1"/>

  </Grid>
</Window>
